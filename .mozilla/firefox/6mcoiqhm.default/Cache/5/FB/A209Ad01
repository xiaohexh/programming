<script type="text/javascript" src="http://static.lecai.com/js/cooperator/common/lottery/quickbuy.js?v=2.9.47.1"></script>
<script type="text/javascript">
(function(){
    var links = document.getElementsByTagName('a');
    var forms = document.getElementsByTagName('form');
    var request = window.location.search.match(/agentId=([^&]+)/);
    if(request == null){
        var request = ["agentId=5530", "5530"];
    }
    if(request != null){
        for(var i = 0; i < links.length; i++) {
            if(/\?/.test(links[i].href) && !/\javascript/.test(links[i].href)) {
                links[i].href= links[i].href + "&"+ request[0];
            }else if(!/\javascript/.test(links[i].href)){
                links[i].href= links[i].href + "?" + request[0];
            }
        }
        for(var i = 0; i < forms.length; i++){
            forms[i].action = forms[i].action + '?' + request[0];
        }
    }
})();

function showGameDateUL(gameUL, index_position) {
    // 滚动赛程事件
    var switchCont = '#switch_cont_' + gameUL;
    var leftBtn = '#left_switch_btn_' + gameUL,
        rightBtn = '#right_switch_btn_' + gameUL;
    var scoreTopPassIndex = index_position;
    var $container = $(switchCont);
    var total = $container.find('ul').length;

    if(total == 0){
        return;
    }
    if($container.find('ul').attr('class') == 'none-ul'){
        $(leftBtn).addClass('left-switch-none');
        $(rightBtn).addClass('right-switch-none');
        return;
    }
    var stepWidth = 316;
    $container.css('width', total * stepWidth + 'px');

    // 显示到指定位置
    function fixPosition(index) {
        var l = -1 * index * stepWidth;
        $container.animate({
            left: l  + 'px'
        },800);
    }

    fixPosition(0);

    // 按钮显示点击
    if(scoreTopPassIndex == 0){
        $(leftBtn).addClass('left-switch-none');
        $(rightBtn).removeClass('right-switch-none');
    }

    if(total <= 3){
        $(leftBtn).addClass('left-switch-none');
        $(rightBtn).addClass('right-switch-none');
    }

    // 绑定事件
    $(rightBtn).unbind().bind('click', function(){
        if(scoreTopPassIndex < total - 3){
            scoreTopPassIndex++
            fixPosition(scoreTopPassIndex);
            $(leftBtn).removeClass('left-switch-none');
        }

        if(scoreTopPassIndex == total - 3){
            $(rightBtn).addClass('right-switch-none');
        }
    });

    $(leftBtn).unbind().bind('click', function(){
        if(scoreTopPassIndex > 0){
            scoreTopPassIndex--;
            fixPosition(scoreTopPassIndex);
            $(rightBtn).removeClass('right-switch-none');
        }
        
        if(scoreTopPassIndex == 0){
            $(leftBtn).addClass('left-switch-none');
        }
    });
}
showGameDateUL('jczq', 0);

$(document).ready(function(){
    /*
    var lotteryJsBaseUrl = 'http://static.lecai.com/js/lottery';
    var quickBuyLotterys = {
        'ssq' : {
            'scripts' : [{'url' : lotteryJsBaseUrl + '/ssq/ssq.base.js' , 'global' : false} , {'url' : lotteryJsBaseUrl + '/ssq/ssq.index.js' , 'global' : false}],
            'lazy' : false
        },
        'dlt' : {
            'scripts' : [{'url' : lotteryJsBaseUrl + '/dlt/dlt.base.js' , 'global' : false} , {'url' : lotteryJsBaseUrl + '/dlt/dlt.index.js' , 'global' : false}],
            'lazy' : false
        }
    };  
    var loadQuickBuyLottery = function(lottery){
        $.ajaxLoadScripts(lottery.scripts);
    };
    for(var l in quickBuyLotterys){
        var lottery = quickBuyLotterys[l];
        if( ! lottery.lazy){
            loadQuickBuyLottery(lottery);
        }
    }
    */
    $('#lottery_tabs span').mouseenter(function(){
        $(this).addClass('now').siblings().removeClass('now');
        var n = $(this).index();
        if(n == 3){
            $($('#lottery_content .cont')[n]).css('visibility', 'visible').siblings().hide();
        }else{
            $($('#lottery_content .cont')[n]).css("display","block");
            $('#lottery_content .cont').each(function(i){
                if(i != n && i != 3){
                    $(this).css("display","none");
                }else if(i==3){
                    $(this).css("visibility","hidden");
                } 
            });
        }
        
        switch($(this).index()+1){
            case 1:
                showGameDateUL('jczq', 0);
                break;
            case 2:
                showGameDateUL('jclq', 0);
                break;
            case 3://数字彩随机操作
              //切换后换随机一注
                ssq_random();
                dlt_random();
                break;
        }
    });
    // 随机生成一注
    $('#dlt_random').click(function(){
        dlt_random();
    });
    $('#ssq_random').click(function(){
        ssq_random();
    });
    //清空
    $('#dlt_clear').click(function(){
        $('#qb_dlt_select .lot_sn_red input[type=text]').val('');
        $('#qb_dlt_select .lot_sn_blue input[type=text]').val('');
    });
    $('#ssq_clear').click(function(){
        $('#qb_ssq_select .lot_sn_red input[type=text]').val('');
        $('#qb_ssq_select .lot_sn_blue input[type=text]').val('');
    });

    var img = document.createElement('img');
    $('a,.qb_submit').mousedown(function(){
        var tongji = $(this).attr('tongji');
        if(tongji){
            img.src = 'http://nsclick.baidu.com/v.gif?pid=107&type=0&m=11&ct=1&c=top&bl=baidulecai&temp=' + new Date().getTime() + "&" + tongji;
        }
    });
});
</script>
